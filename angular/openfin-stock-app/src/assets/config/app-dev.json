{
    "devtools_port": 9090,
    "startup_app": {
        "name": "IG Angular OpenFin-FDC3",
        "description": "IG Angular apps with OpenFin-FDC3 hosted in OpenFin window",
        "uuid": "IgStockDashboardAppID",
        "url": "http://localhost:4200/angular-sample-apps/stocks-dashboard/home",
        "applicationIcon": "http://localhost:4200/angular-sample-apps/stocks-dashboard/assets/favicon.ico",
        "minWidth": 700,
        "minHeight": 155,
        "defaultWidth": 800,
        "defaultHeight": 155,
        "defaultTop": 10,
        "defaultLeft": 10,
        "frame": true,
        "resizable": true,
        "maximizable": true,
        "showTaskbarIcon": true,
        "autoShow": true
    },
    "services": [
        { "name":"layouts" },
        { "name": "fdc3" },
        { "name": "notifications" }
    ],
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing --enable-experimental-web-platform-features",
        "version": "stable"
    },
    "shortcut": {
        "company": "Infragistics",
        "name": "IG Stock Dashboard FDC3",
        "description": "IG Angular Stock Dashboard with OpenFin FDC3",
        "icon": "http://localhost:4200/angular-sample-apps/stocks-dashboard/assets/favicon.ico"
    },
    "splashScreenImage": "http://localhost:4200/angular-sample-apps/stocks-dashboard/assets/splashscreen.png"
}
