{
    "devtools_port": 9090,
    "startup_app": {
        "name": "IG Angular OpenFin-FDC3",
        "description": "IG Angular apps with OpenFin-FDC3 hosted in OpenFin window",
        "uuid": "IgStockDashboardAppID",
        "minWidth": 600,
        "minHeight": 85,
        "defaultWidth": 600,
        "defaultHeight": 85,
        "frame": true,
        "resizable": true,
        "maximizable": true,
        "showTaskbarIcon": true,
        "autoShow" : true,
        "url": "https://infragistics.com/angular-apps/stocks-dashboard/",
        "applicationIcon": "https://infragistics.com/angular-apps/stocks-dashboard/assets/favicon.ico"
    },
    "services": [
        {   "name": "fdc3" }
    ],
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing --enable-experimental-web-platform-features",
        "version": "beta"
    },
    "shortcut": {
        "company": "Infragistics",
        "description": "IG StockChart with FDC3 hosted in OpenFin window",
        "name": "IG Stock Dashboard with OpenFin FDC3",
        "icon": "https://infragistics.com/angular-apps/stocks-dashboard/assets/favicon.ico"
    },
    "splashScreenImage": "https://infragistics.com/angular-apps/stocks-dashboard/assets/splashscreen.png"
}
