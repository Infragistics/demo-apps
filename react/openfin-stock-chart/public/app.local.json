{
    "devtools_port": 9090,
    "startup_app": {
        "name": "IG Stock Chart with FDC3",
        "uuid": "IgStockCharts",
        "minWidth": 300,
        "minHeight": 300,
        "defaultWidth": 900,
        "defaultHeight": 700,
        "frame": true,
        "resizable": true,
        "maximizable": true,
        "showTaskbarIcon": true,
        "url": "http://localhost:3500/index.html",
        "autoShow" : true,
        "applicationIcon": "http://localhost:3500/favicon.ico"
    },
    "services": [
        {   "name": "fdc3" },
        {   "name": "layouts",
            "config": {
              "features": {
                "dock": true,
                "snap": true,
                "tab": false
              }
            }
        }
    ],
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing --enable-experimental-web-platform-features",
        "version": "beta"
    },
    "shortcut": {
        "company": "Infragistics",
        "description": "Infragistics Chart app with dark theme hosted in OpenFin window.",
        "name": "IG Stock Dashboard with OpenFin FDC3",
        "icon": "http://localhost:3500/favicon.ico"
    },
    "splashScreenImage": "http://localhost:3500/images/splashscreen.png"
}
